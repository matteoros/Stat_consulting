Pgt = function(gt, rho) c((1 - rho)^2, 2 * rho * (1 - rho),
rho^2)[gt + 1]
GtAlleles = function(gt) list(c(1), c(1, 2), c(2))[[gt +
1]]
Gt2ordered = function(gt) list(list(c(1, 1)), list(c(1, 2),
c(2, 1)), list(c(2, 2)))[[gt + 1]]
PmendelAllele = function(allOf, gtPar) {
(allOf %in% GtAlleles(gtPar))/length(GtAlleles(gtPar))
}
PmendelOrdered = function(gtOf, gtMo, gtFa) PmendelAllele(gtOf[1],
gtMo) * PmendelAllele(gtOf[2], gtFa)
Pmendel = function(gtOf, gtMo, gtFa) sum(sapply(Gt2ordered(gtOf),
function(gtOf) PmendelOrdered(gtOf, gtMo, gtFa)))
PmendelGtfa = function(gtOf, gtMo, gtFa, rho) Pmendel(gtOf,
gtMo, gtFa) * Pgt(gtFa, rho)
PgfaGgmogof = function(g2, g1, g3, rho) PmendelGtfa(g3, g1,
g2, rho)/sum(sapply(0:2, function(g2P) PmendelGtfa(g3,
g1, g2P, rho)))
DistFaGgmogof = function(g1, g3, rho) sapply(0:2, function(g2) PgfaGgmogof(g2,
g1, g3, rho))
DistFaGgmogof(1, 2, 0.3)
